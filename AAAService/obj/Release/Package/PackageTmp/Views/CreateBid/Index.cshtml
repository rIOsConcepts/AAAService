@model IEnumerable<AAAService.Models.user_viewable_locations_view>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.user_guid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.parentguid)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user_guid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.parentguid)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.location_guid }) |
            @Html.ActionLink("Details", "Details", new { id=item.location_guid }) |
