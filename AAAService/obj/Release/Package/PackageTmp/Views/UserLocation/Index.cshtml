@model IEnumerable<AAAService.Models.user_to_location>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.user_guid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.primary_contact_bit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.locationinfo.name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.user_guid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.primary_contact_bit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.locationinfo.name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.guid }) |
            @Html.ActionLink("Details", "Details", new { id=item.guid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.guid })
        </td>
    </tr>
}

</table>
